<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.deepdragon.entity.weipu.CarouselAdvertisingMapper">

	<sql id="columnList">
		`id`,`tenantId`,concat((select setvalue from shared_system_set where setKey='imageUrl'),concat(concat(groupName,'/'),image)) as `pic`,concat((select setvalue from shared_system_set where setKey='imageUrl'),concat(concat(groupName,'/'),mobileImage)) as `mobilepic`,`groupName`,`image`,`imageWidth`,`imageHight`,`enabled`,`url`,`orderNo`,`description`,`createUser`,`createDate`,`modifyUser`,`modifyDate`,`platform`,`appUrl`,`hybridType`,`hybridUrl`
	</sql>

	<select id="get" resultType="net.deepdragon.entity.weipu.CarouselAdvertising">
		select
		<include refid="columnList" />
		from wp_carousel_advertising
		where id = #{id}
		and tenantId = #{tenantId}
		and platform = #{platform}
	</select>

	<select id="find" resultType="net.deepdragon.entity.weipu.CarouselAdvertising">
		select
		<include refid="columnList" />
		from wp_carousel_advertising
		where tenantId = #{tenantId} and platform = #{platform} and ${property}=#{value}
	</select>

	<select id="findList" resultType="net.deepdragon.entity.weipu.CarouselAdvertising">
		select
		<include refid="columnList" />
		from wp_carousel_advertising
		where tenantId = #{tenantId} and platform = #{platform} and ${property}=#{value}
	</select>
	
	<select id="findListByIds" parameterType="java.lang.String"
		resultType="net.deepdragon.entity.weipu.CarouselAdvertising">
		select
		<include refid="columnList" />
		from wp_carousel_advertising
		where id in
		<foreach collection="list" item="id" index="index" open="("	close=")" separator=",">#{id}</foreach>
		and tenantId = #{tenantId} and platform = #{platform}
	</select>
	
	<select id="getAll" resultType="net.deepdragon.entity.weipu.CarouselAdvertising">
		select
		<include refid="columnList" />
		from wp_carousel_advertising 
		where tenantId = #{tenantId} and platform = #{platform}
	</select>
	
	<select id="getPager" resultType="net.deepdragon.entity.weipu.CarouselAdvertising">
		select
		<include refid="columnList" />
		from wp_carousel_advertising 
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>
	
	<select id="getList" resultType="net.deepdragon.entity.weipu.CarouselAdvertising">
		select
		<include refid="columnList" />
		from wp_carousel_advertising 
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>

	<select id="getTotalCount" resultType="java.lang.Long">
		select count(id) as result
		from wp_carousel_advertising where tenantId = #{tenantId} and platform = #{platform}
	</select>
	
	<select id="isExist" resultType="java.lang.Boolean">
		select count(id) as result from wp_carousel_advertising 
		where tenantId = #{tenantId} and platform = #{platform} and ${property}=#{value}
	</select>
	
	<insert id="save" parameterType="net.deepdragon.entity.weipu.CarouselAdvertising"
		useGeneratedKeys="true" keyProperty="id">
		insert into wp_carousel_advertising
		<trim prefix="(" suffix=")" suffixOverrides=",">
			`id`,`tenantId`,`platform`
			<if test="groupName != null">,`groupName`</if>
			<if test="image != null">,`image`</if>
			<if test="imageWidth != null">,`imageWidth`</if>
			<if test="imageHight != null">,`imageHight`</if>
			<if test="enabled != null">,`enabled`</if>
			<if test="url != null">,`url`</if>
			<if test="orderNo != null">,`orderNo`</if>
			<if test="description != null">,`description`</if>
			<if test="createUser != null">,`createUser`</if>
			<if test="createDate != null">,`createDate`</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			#{id},#{tenantId},#{platform}
			<if test="groupName != null">,#{groupName}</if>
			<if test="image != null">,#{image}</if>
			<if test="imageWidth != null">,#{imageWidth}</if>
			<if test="imageHight != null">,#{imageHight}</if>
			<if test="enabled != null">,#{enabled}</if>
			<if test="url != null">,#{url}</if>
			<if test="orderNo != null">,#{orderNo}</if>
			<if test="description != null">,#{description}</if>
			<if test="createUser != null">,#{createUser}</if>
			<if test="createDate != null">,#{createDate}</if>
		</trim>
	</insert>

	<update id="update" parameterType="net.deepdragon.entity.weipu.CarouselAdvertising">
		update wp_carousel_advertising
		<set>
			`tenantId` = #{tenantId},`platform` = #{platform}
			<if test="groupName != null">,`groupName` = #{groupName}</if>
			<if test="image != null">,`image` = #{image}</if>
			<if test="imageWidth != null">,`imageWidth` = #{imageWidth}</if>
			<if test="imageHight != null">,`imageHight` = #{imageHight}</if>
			<if test="enabled != null">,`enabled` = #{enabled}</if>
			<if test="url != null">,`url` = #{url}</if>
			<if test="orderNo != null">,`orderNo` = #{orderNo}</if>
			<if test="description != null">,`description` = #{description}</if>
			<if test="modifyUser != null">,`modifyUser` = #{modifyUser}</if>
			<if test="modifyDate != null">,`modifyDate` = #{modifyDate}</if>
		</set>
		where id = #{id}
	</update>

	<delete id="delete">
		delete from wp_carousel_advertising
		where id =	#{id}
		and tenantId = #{tenantId}
		and platform = #{platform}
	</delete>
	
</mapper>
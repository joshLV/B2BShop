<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.deepdragon.entity.weipu.OrderJifenMapper">

	<sql id="columnList">
		`id`,`tenantId`,`orderSn`,`payAmount`,`point`,`pointCardNo`,`cardNo`,`createUser`,`createDate`,`modifyUser`,`modifyDate`
	</sql>

	<select id="get" resultType="net.deepdragon.entity.weipu.OrderJifen">
		select
		<include refid="columnList" />
		from wp_order_jifen
		where id = #{id}
	</select>

	<select id="find" resultType="net.deepdragon.entity.weipu.OrderJifen">
		select
		<include refid="columnList" />
		from wp_order_jifen
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>

	<select id="findList" resultType="net.deepdragon.entity.weipu.OrderJifen">
		select
		<include refid="columnList" />
		from wp_order_jifen
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>
	
	<select id="findListByIds" parameterType="java.lang.String"
		resultType="net.deepdragon.entity.weipu.OrderJifen">
		select
		<include refid="columnList" />
		from wp_order_jifen
		where id in
		<foreach collection="list" item="id" index="index" open="("	close=")" separator=",">#{id}</foreach>
	</select>
	
	<select id="getAll" resultType="net.deepdragon.entity.weipu.OrderJifen">
		select
		<include refid="columnList" />
		from wp_order_jifen 
		where tenantId = #{tenantId}
	</select>
	
	<select id="getPager" resultType="net.deepdragon.entity.weipu.OrderJifen">
		select
		<include refid="columnList" />
		from wp_order_jifen 
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>
	
	<select id="getList" resultType="net.deepdragon.entity.weipu.OrderJifen">
		select
		<include refid="columnList" />
		from wp_order_jifen 
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>

	<select id="getTotalCount" resultType="java.lang.Long">
		select count(id) as result
		from wp_order_jifen where tenantId = #{tenantId}
	</select>
	
	<select id="isExist" resultType="java.lang.Boolean">
		select count(id) as result from wp_order_jifen 
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>
	
	<insert id="save" parameterType="net.deepdragon.entity.weipu.OrderJifen"
		useGeneratedKeys="true" keyProperty="id">
		insert into wp_order_jifen
		<trim prefix="(" suffix=")" suffixOverrides=",">
			`id`,`tenantId`
			<if test="orderSn != null">,`orderSn`</if>
			<if test="payAmount != null">,`payAmount`</if>
			<if test="point != null">,`point`</if>
			<if test="pointCardNo != null">,`pointCardNo`</if>
			<if test="cardNo != null">,`cardNo`</if>
			<if test="createUser != null">,`createUser`</if>
			<if test="createDate != null">,`createDate`</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			#{id},#{tenantId}
			<if test="orderSn != null">,#{orderSn}</if>
			<if test="payAmount != null">,#{payAmount}</if>
			<if test="point != null">,#{point}</if>
			<if test="pointCardNo != null">,#{pointCardNo}</if>
			<if test="cardNo != null">,#{cardNo}</if>
			<if test="createUser != null">,#{createUser}</if>
			<if test="createDate != null">,#{createDate}</if>
		</trim>
	</insert>

	<update id="update" parameterType="net.deepdragon.entity.weipu.OrderJifen">
		update wp_order_jifen
		<set>
			`tenantId` = #{tenantId}
			<if test="orderSn != null">,`orderSn` = #{orderSn}</if>
			<if test="payAmount != null">,`payAmount` = #{payAmount}</if>
			<if test="point != null">,`point` = #{point}</if>
			<if test="pointCardNo != null">,`pointCardNo` = #{pointCardNo}</if>
			<if test="cardNo != null">,`cardNo` = #{cardNo}</if>
			<if test="modifyUser != null">,`modifyUser` = #{modifyUser}</if>
			<if test="modifyDate != null">,`modifyDate` = #{modifyDate}</if>
		</set>
		where id = #{id}
	</update>

	<delete id="delete">
		delete from wp_order_jifen
		where id =	#{id}
	</delete>
	
</mapper>
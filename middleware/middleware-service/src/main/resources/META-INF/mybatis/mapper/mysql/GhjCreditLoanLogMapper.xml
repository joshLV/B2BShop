	</sql>

	<select id="get" resultType="net.deepdragon.entity.weipu.GhjCreditLoanLog">
		select
		<include refid="columnList" />
		from ghj_credit_loan_log
		where loanLogId = #{id}
	</select>

	<select id="find" resultType="net.deepdragon.entity.weipu.GhjCreditLoanLog">
		select
		<include refid="columnList" />
		from ghj_credit_loan_log
		where tenantId = #{tenantId} and ${property}=#{value}

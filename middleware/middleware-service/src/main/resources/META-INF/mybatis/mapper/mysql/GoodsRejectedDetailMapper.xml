<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.deepdragon.entity.weipu.GoodsRejectedDetailMapper">

	<sql id="columnList">
		`id`,`tenantId`,`rejectedSn`,`orderItemId`,`goodsId`,`goodsQuantity`,`rejectedMoney`,`rejectedReason`,`remark`,`createUser`,`createDate`,`modifyUser`,`modifyDate`
	</sql>

	<select id="get" resultType="net.deepdragon.entity.weipu.GoodsRejectedDetail">
		select
		<include refid="columnList" />
		from wp_goods_rejected_detail
		where id = #{id}
	</select>

	<select id="find" resultType="net.deepdragon.entity.weipu.GoodsRejectedDetail">
		select
		<include refid="columnList" />
		from wp_goods_rejected_detail
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>

	<select id="findList" resultType="net.deepdragon.entity.weipu.GoodsRejectedDetail">
		select
		<include refid="columnList" />
		from wp_goods_rejected_detail
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>
	
	<select id="findListByIds" parameterType="java.lang.String"
		resultType="net.deepdragon.entity.weipu.GoodsRejectedDetail">
		select
		<include refid="columnList" />
		from wp_goods_rejected_detail
		where id in
		<foreach collection="list" item="id" index="index" open="("	close=")" separator=",">#{id}</foreach>
	</select>
	
	<select id="getAll" resultType="net.deepdragon.entity.weipu.GoodsRejectedDetail">
		select
		<include refid="columnList" />
		from wp_goods_rejected_detail 
		where tenantId = #{tenantId}
	</select>
	
	<select id="getPager" resultType="net.deepdragon.entity.weipu.GoodsRejectedDetail">
		select
		<include refid="columnList" />
		from wp_goods_rejected_detail 
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>
	
	<select id="getList" resultType="net.deepdragon.entity.weipu.GoodsRejectedDetail">
		select
		<include refid="columnList" />
		from wp_goods_rejected_detail 
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>

	<select id="getTotalCount" resultType="java.lang.Long">
		select count(id) as result
		from wp_goods_rejected_detail where tenantId = #{tenantId}
	</select>
	
	<select id="isExist" resultType="java.lang.Boolean">
		select count(id) as result from wp_goods_rejected_detail 
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>
	
	<insert id="save" parameterType="net.deepdragon.entity.weipu.GoodsRejectedDetail"
		useGeneratedKeys="true" keyProperty="id">
		insert into wp_goods_rejected_detail
		<trim prefix="(" suffix=")" suffixOverrides=",">
			`id`,`tenantId`
			<if test="rejectedSn != null">,`rejectedSn`</if>
			<if test="orderItemId != null">,`orderItemId`</if>
			<if test="goodsId != null">,`goodsId`</if>
			<if test="goodsQuantity != null">,`goodsQuantity`</if>
			<if test="rejectedMoney != null">,`rejectedMoney`</if>
			<if test="rejectedReason != null">,`rejectedReason`</if>
			<if test="remark != null">,`remark`</if>
			<if test="createUser != null">,`createUser`</if>
			<if test="createDate != null">,`createDate`</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			#{id},#{tenantId}
			<if test="rejectedSn != null">,#{rejectedSn}</if>
			<if test="orderItemId != null">,#{orderItemId}</if>
			<if test="goodsId != null">,#{goodsId}</if>
			<if test="goodsQuantity != null">,#{goodsQuantity}</if>
			<if test="rejectedMoney != null">,#{rejectedMoney}</if>
			<if test="rejectedReason != null">,#{rejectedReason}</if>
			<if test="remark != null">,#{remark}</if>
			<if test="createUser != null">,#{createUser}</if>
			<if test="createDate != null">,#{createDate}</if>
		</trim>
	</insert>

	<update id="update" parameterType="net.deepdragon.entity.weipu.GoodsRejectedDetail">
		update wp_goods_rejected_detail
		<set>
			`tenantId` = #{tenantId}
			<if test="rejectedSn != null">,`rejectedSn` = #{rejectedSn}</if>
			<if test="orderItemId != null">,`orderItemId` = #{orderItemId}</if>
			<if test="goodsId != null">,`goodsId` = #{goodsId}</if>
			<if test="goodsQuantity != null">,`goodsQuantity` = #{goodsQuantity}</if>
			<if test="rejectedMoney != null">,`rejectedMoney` = #{rejectedMoney}</if>
			<if test="rejectedReason != null">,`rejectedReason` = #{rejectedReason}</if>
			<if test="remark != null">,`remark` = #{remark}</if>
			<if test="modifyUser != null">,`modifyUser` = #{modifyUser}</if>
			<if test="modifyDate != null">,`modifyDate` = #{modifyDate}</if>
		</set>
		where id = #{id}
	</update>

	<delete id="delete">
		delete from wp_goods_rejected_detail
		where id =	#{id}
	</delete>
	
</mapper>
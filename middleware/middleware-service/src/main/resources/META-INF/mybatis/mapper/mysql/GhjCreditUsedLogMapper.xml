<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.deepdragon.entity.weipu.GhjCreditUsedLogMapper">

	<sql id="columnList">
		`usedLogId` as id,`tenantId`,`platform`,`replyId`,`buyerId`,`amount`,`usedTime`,`remark`,`createUser`,`createDate`,`modifyUser`,`modifyDate`
	</sql>

	<select id="get" resultType="net.deepdragon.entity.weipu.GhjCreditUsedLog">
		select
		<include refid="columnList" />
		from ghj_credit_used_log
		where usedLogId = #{id}
	</select>

	<select id="find" resultType="net.deepdragon.entity.weipu.GhjCreditUsedLog">
		select
		<include refid="columnList" />
		from ghj_credit_used_log
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>

	<select id="findList" resultType="net.deepdragon.entity.weipu.GhjCreditUsedLog">
		select
		<include refid="columnList" />
		from ghj_credit_used_log
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>
	
	<select id="findListByIds" parameterType="java.lang.String"
		resultType="net.deepdragon.entity.weipu.GhjCreditUsedLog">
		select
		<include refid="columnList" />
		from ghj_credit_used_log
		where usedLogId in
		<foreach collection="list" item="id" index="index" open="("	close=")" separator=",">#{id}</foreach>
	</select>
	
	<select id="getAll" resultType="net.deepdragon.entity.weipu.GhjCreditUsedLog">
		select
		<include refid="columnList" />
		from ghj_credit_used_log
		where tenantId = #{tenantId}
	</select>
	
	<select id="getPager" resultType="net.deepdragon.entity.weipu.GhjCreditUsedLog">
		select cl.*,cr.bankName,cr.bankDept,cr.creditAmount,cr.totalAmount,cr.usedAmount from ghj_credit_used_log cl LEFT JOIN ghj_credit_reply  cr on cl.replyId=cr.replyId
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>
	
	<select id="getList" resultType="net.deepdragon.entity.weipu.GhjCreditUsedLog">
		select
		<include refid="columnList" />
		from ghj_credit_used_log
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>

	<select id="getTotalCount" resultType="java.lang.Long">
		select count(usedLogId) as result
		from ghj_credit_used_log where tenantId = #{tenantId}
	</select>
	
	<select id="isExist" resultType="java.lang.Boolean">
		select count(usedLogId) as result from ghj_credit_used_log
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>
	
	<insert id="save" parameterType="net.deepdragon.entity.weipu.GhjCreditUsedLog"
		useGeneratedKeys="true" keyProperty="id">
		insert into ghj_credit_used_log
		<trim prefix="(" suffix=")" suffixOverrides=",">
			`usedLogId`,`tenantId`
			<if test="platform != null">,`platform`</if>
			<if test="replyId != null">,`replyId`</if>
			<if test="buyerId != null">,`buyerId`</if>
			<if test="amount != null">,`amount`</if>
			<if test="usedTime != null">,`usedTime`</if>
			<if test="remark != null">,`remark`</if>
			<if test="createUser != null">,`createUser`</if>
			<if test="createDate != null">,`createDate`</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			#{id},#{tenantId}
			<if test="platform != null">,#{platform}</if>
			<if test="replyId != null">,#{replyId}</if>
			<if test="buyerId != null">,#{buyerId}</if>
			<if test="amount != null">,#{amount}</if>
			<if test="usedTime != null">,#{usedTime}</if>
			<if test="remark != null">,#{remark}</if>
			<if test="createUser != null">,#{createUser}</if>
			<if test="createDate != null">,#{createDate}</if>
		</trim>
	</insert>

	<update id="update" parameterType="net.deepdragon.entity.weipu.GhjCreditUsedLog">
		update ghj_credit_used_log
		<set>
			`tenantId` = #{tenantId}
			<if test="platform != null">,`platform` = #{platform}</if>
			<if test="replyId != null">,`replyId` = #{replyId}</if>
			<if test="buyerId != null">,`buyerId` = #{buyerId}</if>
			<if test="amount != null">,`amount` = #{amount}</if>
			<if test="usedTime != null">,`usedTime` = #{usedTime}</if>
			<if test="remark != null">,`remark` = #{remark}</if>
			<if test="modifyUser != null">,`modifyUser` = #{modifyUser}</if>
			<if test="modifyDate != null">,`modifyDate` = #{modifyDate}</if>
		</set>
		where usedLogId = #{id}
	</update>

	<delete id="delete">
		delete from ghj_credit_used_log
		where usedLogId =	#{id}
	</delete>
	
</mapper>
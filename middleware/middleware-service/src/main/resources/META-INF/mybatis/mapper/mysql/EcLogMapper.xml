<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.deepdragon.entity.weipu.EcLogMapper">

	<sql id="columnList">
		`id`,`tenantId`,`userName`,`userNamecn`,`logType`,`logIp`,`logBeizhu`,`browserType`,`createUser`,`createDate`,`modifyUser`,`modifyDate`
	</sql>

	<select id="get" resultType="net.deepdragon.entity.weipu.EcLog">
		select
		<include refid="columnList" />
		from sys_ec_log
		where id = #{id}
	</select>

	<select id="find" resultType="net.deepdragon.entity.weipu.EcLog">
		select
		<include refid="columnList" />
		from sys_ec_log
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>

	<select id="findList" resultType="net.deepdragon.entity.weipu.EcLog">
		select
		<include refid="columnList" />
		from sys_ec_log
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>
	
	<select id="findListByIds" parameterType="java.lang.String"
		resultType="net.deepdragon.entity.weipu.EcLog">
		select
		<include refid="columnList" />
		from sys_ec_log
		where id in
		<foreach collection="list" item="id" index="index" open="("	close=")" separator=",">#{id}</foreach>
	</select>
	
	<select id="getAll" resultType="net.deepdragon.entity.weipu.EcLog">
		select
		<include refid="columnList" />
		from sys_ec_log 
		where tenantId = #{tenantId}
	</select>
	
	<select id="getPager" resultType="net.deepdragon.entity.weipu.EcLog">
		select
		<include refid="columnList" />
		from sys_ec_log 
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>
	
	<select id="getList" resultType="net.deepdragon.entity.weipu.EcLog">
		select
		<include refid="columnList" />
		from sys_ec_log 
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>

	<select id="getTotalCount" resultType="java.lang.Long">
		select count(id) as result
		from sys_ec_log where tenantId = #{tenantId}
	</select>
	
	<select id="isExist" resultType="java.lang.Boolean">
		select count(id) as result from sys_ec_log 
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>
	
	<insert id="save" parameterType="net.deepdragon.entity.weipu.EcLog"
		useGeneratedKeys="true" keyProperty="id">
		insert into sys_ec_log
		<trim prefix="(" suffix=")" suffixOverrides=",">
			`id`,`tenantId`
			<if test="userName!=null and userName!=''">,`userName`</if>
			<if test="userNamecn!=null and userNamecn!=''">,`userNamecn`</if>
			<if test="logType!=null and logType!=''">,`logType`</if>
			<if test="logIp!=null and logIp!=''">,`logIp`</if>
			<if test="logBeizhu!=null and logBeizhu!=''">,`logBeizhu`</if>
			<if test="browserType!=null and browserType!=''">,`browserType`</if>
			<if test="createUser!=null and  createUser!=''">,`createUser`</if>
			<if test="createDate!=null and  createDate!=''">,`createDate`</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			#{id}
			,#{tenantId}
			<if test="userName!=null and userName!=''">,#{userName}</if>
			<if test="userNamecn!=null and userNamecn!=''">,#{userNamecn}</if>
			<if test="logType!=null and logType!=''">,#{logType}</if>
			<if test="logIp!=null and logIp!=''">,#{logIp}</if>
			<if test="logBeizhu!=null and logBeizhu!=''">,#{logBeizhu}</if>
			<if test="browserType!=null and browserType!=''">,#{browserType}</if>
			<if test="createUser!=null and createUser!=''">,#{createUser}</if>
			<if test="createDate!=null and createDate!=''">,#{createDate}</if>
		</trim>
	</insert>

	<update id="update" parameterType="net.deepdragon.entity.weipu.EcLog">
		update sys_ec_log
		<set>
			`tenantId` = #{tenantId}
			<if test="userName != null">,`userName` = #{userName}</if>
			<if test="userNamecn != null">,`userNamecn` = #{userNamecn}</if>
			<if test="logType != null">,`logType` = #{logType}</if>
			<if test="logIp != null">,`logIp` = #{logIp}</if>
			<if test="logBeizhu != null">,`logBeizhu`</if>
			<if test="browserType != null">,`browserType`</if>
			<if test="modifyUser != null">,`modifyUser` = #{modifyUser}</if>
			<if test="modifyDate != null">,`modifyDate` = #{modifyDate}</if>
		</set>
		where id = #{id}
	</update>

	<delete id="delete">
		delete from sys_ec_log
		where id =	#{id}
	</delete>
	
</mapper>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.deepdragon.entity.weipu.GhjCreditMapper">

	<sql id="columnList">
		`creditId` as id,`tenantId`,`platform`,`buyerId`,`operator`,`contacts`,`applyAmount`,`mobile`,`applyTime`,`compDesc`,`dealTime`,`status`,`remark`,`createUser`,`createDate`,`modifyUser`,`modifyDate`
	</sql>

	<select id="get" resultType="net.deepdragon.entity.weipu.GhjCredit">
		select
		<include refid="columnList" />
		from ghj_credit
		where creditId = #{id}
	</select>

	<select id="find" resultType="net.deepdragon.entity.weipu.GhjCredit">
		select
		<include refid="columnList" />
		from ghj_credit
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>

	<select id="findList" resultType="net.deepdragon.entity.weipu.GhjCredit">
		select
		<include refid="columnList" />
		from ghj_credit
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>
	
	<select id="findListByIds" parameterType="java.lang.String"
		resultType="net.deepdragon.entity.weipu.GhjCredit">
		select
		<include refid="columnList" />
		from ghj_credit
		where creditId in
		<foreach collection="list" item="id" index="index" open="("	close=")" separator=",">#{id}</foreach>
	</select>
	
	<select id="getAll" resultType="net.deepdragon.entity.weipu.GhjCredit">
		select
		<include refid="columnList" />
		from ghj_credit
		where tenantId = #{tenantId}
	</select>
	
	<select id="getPager" resultType="net.deepdragon.entity.weipu.GhjCredit">
		select
		cre.creditId as id,cre.tenantId,cre.platform,cre.buyerId,cre.operator,cre.contacts,
		cre.applyAmount,cre.mobile,cre.applyTime,cre.compDesc,cre.dealTime,cre.status,cre.remark,
		cre.createUser,cre.createDate,cre.modifyUser,cre.modifyDate,cr.replyId
		from ghj_credit cre left join ghj_credit_reply cr on cre.creditId= cr.creditId
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>
	
	<select id="getList" resultType="net.deepdragon.entity.weipu.GhjCredit">
		select
		<include refid="columnList" />
		from ghj_credit
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>

	<select id="getTotalCount" resultType="java.lang.Long">
		select count(creditId) as result
		from ghj_credit where tenantId = #{tenantId}
	</select>
	
	<select id="isExist" resultType="java.lang.Boolean">
		select count(creditId) as result from ghj_credit
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>
	
	<insert id="save" parameterType="net.deepdragon.entity.weipu.GhjCredit"
		useGeneratedKeys="true" keyProperty="id">
		insert into ghj_credit
		<trim prefix="(" suffix=")" suffixOverrides=",">
			`creditId`,`tenantId`
			<if test="platform != null">,`platform`</if>
			<if test="buyerId != null">,`buyerId`</if>
			<if test="operator != null">,`operator`</if>
			<if test="contacts != null">,`contacts`</if>
			<if test="applyAmount != null">,`applyAmount`</if>
			<if test="mobile != null">,`mobile`</if>
			<if test="applyTime != null">,`applyTime`</if>
			<if test="compDesc != null">,`compDesc`</if>
			<if test="dealTime != null">,`dealTime`</if>
			<if test="status != null">,`status`</if>
			<if test="remark != null">,`remark`</if>
			<if test="createUser != null">,`createUser`</if>
			<if test="createDate != null">,`createDate`</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			#{id},#{tenantId}
			<if test="platform != null">,#{platform}</if>
			<if test="buyerId != null">,#{buyerId}</if>
			<if test="operator != null">,#{operator}</if>
			<if test="contacts != null">,#{contacts}</if>
			<if test="applyAmount != null">,#{applyAmount}</if>
			<if test="mobile != null">,#{mobile}</if>
			<if test="applyTime != null">,#{applyTime}</if>
			<if test="compDesc != null">,#{compDesc}</if>
			<if test="dealTime != null">,#{dealTime}</if>
			<if test="status != null">,#{status}</if>
			<if test="remark != null">,#{remark}</if>
			<if test="createUser != null">,#{createUser}</if>
			<if test="createDate != null">,#{createDate}</if>
		</trim>
	</insert>

	<update id="update" parameterType="net.deepdragon.entity.weipu.GhjCredit">
		update ghj_credit
		<set>
			`tenantId` = #{tenantId}
			<if test="platform != null">,`platform` = #{platform}</if>
			<if test="buyerId != null">,`buyerId` = #{buyerId}</if>
			<if test="operator != null">,`operator` = #{operator}</if>
			<if test="contacts != null">,`contacts` = #{contacts}</if>
			<if test="applyAmount != null">,`applyAmount` = #{applyAmount}</if>
			<if test="mobile != null">,`mobile` = #{mobile}</if>
			<if test="applyTime != null">,`applyTime` = #{applyTime}</if>
			<if test="compDesc != null">,`compDesc` = #{compDesc}</if>
			<if test="dealTime != null">,`dealTime` = #{dealTime}</if>
			<if test="status != null">,`status` = #{status}</if>
			<if test="remark != null">,`remark` = #{remark}</if>
			<if test="modifyUser != null">,`modifyUser` = #{modifyUser}</if>
			<if test="modifyDate != null">,`modifyDate` = #{modifyDate}</if>
		</set>
		where creditId = #{id}
	</update>

	<delete id="delete">
		delete from ghj_credit
		where creditId =	#{id}
	</delete>
	
</mapper>
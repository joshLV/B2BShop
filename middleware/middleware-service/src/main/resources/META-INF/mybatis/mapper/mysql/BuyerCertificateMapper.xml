<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.deepdragon.entity.weipu.BuyerCertificateMapper">

	<sql id="columnList">
		`id`,`tenantId`,`buyerId`,`businessLicense`,`openingPermit`,`taxRegistrationCertificate`,`organizationCodeCertificate`,`openingBank`,`bankNumber`,`isDelete`,`createUser`,`createDate`,`modifyUser`,`modifyDate`,`groupName`
	</sql>

	<select id="get" resultType="net.deepdragon.entity.weipu.BuyerCertificate">
		select
		<include refid="columnList" />
		from b2b_buyer_certificate
		where id = #{id}
	</select>

	<select id="find" resultType="net.deepdragon.entity.weipu.BuyerCertificate">
		select
		<include refid="columnList" />
		from b2b_buyer_certificate
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>

	<select id="findList" resultType="net.deepdragon.entity.weipu.BuyerCertificate">
		select
		<include refid="columnList" />
		from b2b_buyer_certificate
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>
	
	<select id="findListByIds" parameterType="java.lang.String"
		resultType="net.deepdragon.entity.weipu.BuyerCertificate">
		select
		<include refid="columnList" />
		from b2b_buyer_certificate
		where id in
		<foreach collection="list" item="id" index="index" open="("	close=")" separator=",">#{id}</foreach>
	</select>
	
	<select id="getAll" resultType="net.deepdragon.entity.weipu.BuyerCertificate">
		select
		<include refid="columnList" />
		from b2b_buyer_certificate 
		where tenantId = #{tenantId}
	</select>
	
	<select id="getPager" resultType="net.deepdragon.entity.weipu.BuyerCertificate">
		select
		<include refid="columnList" />
		from b2b_buyer_certificate 
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>
	
	<select id="getList" resultType="net.deepdragon.entity.weipu.BuyerCertificate">
		select
		<include refid="columnList" />
		from b2b_buyer_certificate 
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>

	<select id="getTotalCount" resultType="java.lang.Long">
		select count(id) as result
		from b2b_buyer_certificate where tenantId = #{tenantId}
	</select>
	
	<select id="isExist" resultType="java.lang.Boolean">
		select count(id) as result from b2b_buyer_certificate 
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>
	
	<insert id="save" parameterType="net.deepdragon.entity.weipu.BuyerCertificate"
		useGeneratedKeys="true" keyProperty="id">
		insert into b2b_buyer_certificate
		<trim prefix="(" suffix=")" suffixOverrides=",">
			`id`,`tenantId`
			<if test="buyerId != null">,`buyerId`</if>
			<if test="businessLicense != null">,`businessLicense`</if>
			<if test="openingPermit != null">,`openingPermit`</if>
			<if test="taxRegistrationCertificate != null">,`taxRegistrationCertificate`</if>
			<if test="organizationCodeCertificate != null">,`organizationCodeCertificate`</if>
			<if test="openingBank != null">,`openingBank`</if>
			<if test="bankNumber != null">,`bankNumber`</if>
			<if test="isDelete != null">,`isDelete`</if>
			<if test="createUser != null">,`createUser`</if>
			<if test="createDate != null">,`createDate`</if>
			<if test="groupName != null">,`groupName`</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			#{id},#{tenantId}
			<if test="buyerId != null">,#{buyerId}</if>
			<if test="businessLicense != null">,#{businessLicense}</if>
			<if test="openingPermit != null">,#{openingPermit}</if>
			<if test="taxRegistrationCertificate != null">,#{taxRegistrationCertificate}</if>
			<if test="organizationCodeCertificate != null">,#{organizationCodeCertificate}</if>
			<if test="openingBank != null">,#{openingBank}</if>
			<if test="bankNumber != null">,#{bankNumber}</if>
			<if test="isDelete != null">,#{isDelete}</if>
			<if test="createUser != null">,#{createUser}</if>
			<if test="createDate != null">,#{createDate}</if>
			<if test="groupName != null">,`groupName`</if>
		</trim>
	</insert>

	<update id="update" parameterType="net.deepdragon.entity.weipu.BuyerCertificate">
		update b2b_buyer_certificate
		<set>
			`tenantId` = #{tenantId}
			<if test="buyerId != null">,`buyerId` = #{buyerId}</if>
			<if test="businessLicense != null">,`businessLicense` = #{businessLicense}</if>
			<if test="openingPermit != null">,`openingPermit` = #{openingPermit}</if>
			<if test="taxRegistrationCertificate != null">,`taxRegistrationCertificate` = #{taxRegistrationCertificate}</if>
			<if test="organizationCodeCertificate != null">,`organizationCodeCertificate` = #{organizationCodeCertificate}</if>
			<if test="openingBank != null">,`openingBank` = #{openingBank}</if>
			<if test="bankNumber != null">,`bankNumber` = #{bankNumber}</if>
			<if test="isDelete != null">,`isDelete` = #{isDelete}</if>
			<if test="modifyUser != null">,`modifyUser` = #{modifyUser}</if>
			<if test="modifyDate != null">,`modifyDate` = #{modifyDate}</if>
			<if test="groupName != null">,`groupName` = #{groupName}</if>
		</set>
		where id = #{id}
	</update>

	<delete id="delete">
		delete from b2b_buyer_certificate
		where id =	#{id}
	</delete>
	
</mapper>
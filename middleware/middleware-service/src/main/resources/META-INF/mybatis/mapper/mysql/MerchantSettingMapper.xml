<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.deepdragon.entity.weipu.MerchantSettingMapper">

	<sql id="columnList">
		`id`,`tenantId`,`shopId`,`setvalue`,`setkey`
	</sql>

	<select id="get" resultType="net.deepdragon.entity.weipu.MerchantSetting">
		select
		<include refid="columnList" />
		from wp_merchant_setting
		where id = #{id}
	</select>

	<select id="find" resultType="net.deepdragon.entity.weipu.MerchantSetting">
		select
		<include refid="columnList" />
		from wp_merchant_setting
		where tenantId = #{tenantId} and
		${property}=#{value}
	</select>

	<select id="findList" resultType="net.deepdragon.entity.weipu.MerchantSetting">
		select
		<include refid="columnList" />
		from wp_merchant_setting
		where tenantId = #{tenantId} and
		${property}=#{value}
	</select>

	<select id="findListByIds" parameterType="java.lang.String"
		resultType="net.deepdragon.entity.weipu.MerchantSetting">
		select
		<include refid="columnList" />
		from wp_merchant_setting
		where id in
		<foreach collection="list" item="id" index="index" open="("
			close=")" separator=",">#{id}</foreach>
	</select>

	<select id="getAll" resultType="net.deepdragon.entity.weipu.MerchantSetting">
		select
		<include refid="columnList" />
		from wp_merchant_setting
		where tenantId = #{tenantId}
	</select>

	<select id="getPager" resultType="net.deepdragon.entity.weipu.MerchantSetting">
		select
		<include refid="columnList" />
		from wp_merchant_setting
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>

	<select id="getList" resultType="net.deepdragon.entity.weipu.MerchantSetting">
		select
		<include refid="columnList" />
		from wp_merchant_setting
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>

	<select id="getTotalCount" resultType="java.lang.Long">
		select count(id) as result
		from wp_merchant_setting where tenantId = #{tenantId}
	</select>

	<select id="isExist" resultType="java.lang.Boolean">
		select count(id) as result from
		wp_merchant_setting
		where tenantId = #{tenantId} and
		${property}=#{value}
	</select>

	<insert id="save" parameterType="net.deepdragon.entity.weipu.MerchantSetting"
		useGeneratedKeys="true" keyProperty="id">
		insert into wp_merchant_setting
		<trim prefix="(" suffix=")" suffixOverrides=",">
			`id`,`tenantId`
			<if test="shopId != null">,`shopId`</if>
			<if test="setvalue != null">,`setvalue`</if>
			<if test="setkey != null">,`setkey`</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			#{id},#{tenantId}
			<if test="shopId != null">,#{shopId}</if>
			<if test="setvalue != null">,#{setvalue}</if>
			<if test="setkey != null">,#{setkey}</if>
		</trim>
	</insert>

	<update id="update" parameterType="net.deepdragon.entity.weipu.MerchantSetting">
		update wp_merchant_setting
		<set>
			`tenantId` = #{tenantId}
			<if test="shopId != null">,`shopId` = #{shopId}</if>
			<if test="setvalue != null">,`setvalue` = #{setvalue}</if>
			<if test="setkey != null">,`setkey` = #{setkey}</if>
		</set>
		where id = #{id}
	</update>

	<delete id="delete">
		delete from wp_merchant_setting
		where id = #{id}
	</delete>

	<delete id="deleteAll">
		delete from wp_merchant_setting
		where shopId =#{shopId}
	</delete>

</mapper>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.deepdragon.entity.weipu.DeliveryManMapper">

	<sql id="columnList">
		`id`,`tenantId`,`userName`,`passwd`,`deviceId`,`name`,`nickName`,`sex`,`mobile`,`phone`,`birthday`,`status`,`createUser`,`createDate`,`modifyUser`,`modifyDate`
	</sql>

	<select id="get" resultType="net.deepdragon.entity.weipu.DeliveryMan">
		select
		<include refid="columnList" />
		from wp_delivery_man
		where id = #{id}
	</select>

	<select id="find" resultType="net.deepdragon.entity.weipu.DeliveryMan">
		select
		<include refid="columnList" />
		from wp_delivery_man
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>

	<select id="findList" resultType="net.deepdragon.entity.weipu.DeliveryMan">
		select
		<include refid="columnList" />
		from wp_delivery_man
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>
	
	<select id="findListByIds" parameterType="java.lang.String"
		resultType="net.deepdragon.entity.weipu.DeliveryMan">
		select
		<include refid="columnList" />
		from wp_delivery_man
		where id in
		<foreach collection="list" item="id" index="index" open="("	close=")" separator=",">#{id}</foreach>
	</select>
	
	<select id="getAll" resultType="net.deepdragon.entity.weipu.DeliveryMan">
		select
		<include refid="columnList" />
		from wp_delivery_man 
		where tenantId = #{tenantId}
	</select>
	
	<select id="getPager" resultType="net.deepdragon.entity.weipu.DeliveryMan">
		select
		<include refid="columnList" />
		from wp_delivery_man 
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>
	
	<select id="getList" resultType="net.deepdragon.entity.weipu.DeliveryMan">
		select
		<include refid="columnList" />
		from wp_delivery_man 
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>

	<select id="getTotalCount" resultType="java.lang.Long">
		select count(id) as result
		from wp_delivery_man where tenantId = #{tenantId}
	</select>
	
	<select id="isExist" resultType="java.lang.Boolean">
		select count(id) as result from wp_delivery_man 
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>
	
	<insert id="save" parameterType="net.deepdragon.entity.weipu.DeliveryMan"
		useGeneratedKeys="true" keyProperty="id">
		insert into wp_delivery_man
		<trim prefix="(" suffix=")" suffixOverrides=",">
			`id`,`tenantId`
			<if test="userName != null">,`userName`</if>
			<if test="passwd != null">,`passwd`</if>
			<if test="deviceId != null">,`deviceId`</if>
			<if test="name != null">,`name`</if>
			<if test="nickName != null">,`nickName`</if>
			<if test="sex != null">,`sex`</if>
			<if test="mobile != null">,`mobile`</if>
			<if test="phone != null">,`phone`</if>
			<if test="birthday != null">,`birthday`</if>
			<if test="status != null">,`status`</if>
			<if test="createUser != null">,`createUser`</if>
			<if test="createDate != null">,`createDate`</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			#{id},#{tenantId}
			<if test="userName != null">,#{userName}</if>
			<if test="passwd != null">,#{passwd}</if>
			<if test="deviceId != null">,#{deviceId}</if>
			<if test="name != null">,#{name}</if>
			<if test="nickName != null">,#{nickName}</if>
			<if test="sex != null">,#{sex}</if>
			<if test="mobile != null">,#{mobile}</if>
			<if test="phone != null">,#{phone}</if>
			<if test="birthday != null">,#{birthday}</if>
			<if test="status != null">,#{status}</if>
			<if test="createUser != null">,#{createUser}</if>
			<if test="createDate != null">,#{createDate}</if>
		</trim>
	</insert>

	<update id="update" parameterType="net.deepdragon.entity.weipu.DeliveryMan">
		update wp_delivery_man
		<set>
			`tenantId` = #{tenantId}
			<if test="userName != null">,`userName` = #{userName}</if>
			<if test="passwd != null">,`passwd` = #{passwd}</if>
			<if test="deviceId != null">,`deviceId` = #{deviceId}</if>
			<if test="name != null">,`name` = #{name}</if>
			<if test="nickName != null">,`nickName` = #{nickName}</if>
			<if test="sex != null">,`sex` = #{sex}</if>
			<if test="mobile != null">,`mobile` = #{mobile}</if>
			<if test="phone != null">,`phone` = #{phone}</if>
			<if test="birthday != null">,`birthday` = #{birthday}</if>
			<if test="status != null">,`status` = #{status}</if>
			<if test="modifyUser != null">,`modifyUser` = #{modifyUser}</if>
			<if test="modifyDate != null">,`modifyDate` = #{modifyDate}</if>
		</set>
		where id = #{id}
	</update>

	<delete id="delete">
		delete from wp_delivery_man
		where id =	#{id}
	</delete>
	
</mapper>
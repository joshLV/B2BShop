<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.deepdragon.entity.weipu.BuyerAuditMapper">

	<sql id="columnList">
		`id`,`tenantId`,`buyerId`,`auditUser`,`auditTime`,`auditOpinion`,`auditStatus`,`isDelete`,`createUser`,`createDate`,`modifyUser`,`modifyDate`
	</sql>

	<select id="get" resultType="net.deepdragon.entity.weipu.BuyerAudit">
		select
		<include refid="columnList" />
		from b2b_buyer_audit
		where id = #{id}
	</select>

	<select id="find" resultType="net.deepdragon.entity.weipu.BuyerAudit">
		select
		<include refid="columnList" />
		from b2b_buyer_audit
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>

	<select id="findList" resultType="net.deepdragon.entity.weipu.BuyerAudit">
		select
		<include refid="columnList" />
		from b2b_buyer_audit
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>
	
	<select id="findListByIds" parameterType="java.lang.String"
		resultType="net.deepdragon.entity.weipu.BuyerAudit">
		select
		<include refid="columnList" />
		from b2b_buyer_audit
		where id in
		<foreach collection="list" item="id" index="index" open="("	close=")" separator=",">#{id}</foreach>
	</select>
	
	<select id="getAll" resultType="net.deepdragon.entity.weipu.BuyerAudit">
		select
		<include refid="columnList" />
		from b2b_buyer_audit 
		where tenantId = #{tenantId}
	</select>
	
	<select id="getPager" resultType="net.deepdragon.entity.weipu.BuyerAudit">
		select
		<include refid="columnList" />
		from b2b_buyer_audit 
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>
	
	<select id="getList" resultType="net.deepdragon.entity.weipu.BuyerAudit">
		select
		<include refid="columnList" />
		from b2b_buyer_audit 
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>

	<select id="getTotalCount" resultType="java.lang.Long">
		select count(id) as result
		from b2b_buyer_audit where tenantId = #{tenantId}
	</select>
	
	<select id="isExist" resultType="java.lang.Boolean">
		select count(id) as result from b2b_buyer_audit 
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>
	
	<insert id="save" parameterType="net.deepdragon.entity.weipu.BuyerAudit"
		useGeneratedKeys="true" keyProperty="id">
		insert into b2b_buyer_audit
		<trim prefix="(" suffix=")" suffixOverrides=",">
			`id`,`tenantId`
			<if test="buyerId != null">,`buyerId`</if>
			<if test="auditUser != null">,`auditUser`</if>
			<if test="auditTime != null">,`auditTime`</if>
			<if test="auditOpinion != null">,`auditOpinion`</if>
			<if test="auditStatus != null">,`auditStatus`</if>
			<if test="isDelete != null">,`isDelete`</if>
			<if test="createUser != null">,`createUser`</if>
			<if test="createDate != null">,`createDate`</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			#{id},#{tenantId}
			<if test="buyerId != null">,#{buyerId}</if>
			<if test="auditUser != null">,#{auditUser}</if>
			<if test="auditTime != null">,#{auditTime}</if>
			<if test="auditOpinion != null">,#{auditOpinion}</if>
			<if test="auditStatus != null">,#{auditStatus}</if>
			<if test="isDelete != null">,#{isDelete}</if>
			<if test="createUser != null">,#{createUser}</if>
			<if test="createDate != null">,#{createDate}</if>
		</trim>
	</insert>

	<update id="update" parameterType="net.deepdragon.entity.weipu.BuyerAudit">
		update b2b_buyer_audit
		<set>
			`tenantId` = #{tenantId}
			<if test="buyerId != null">,`buyerId` = #{buyerId}</if>
			<if test="auditUser != null">,`auditUser` = #{auditUser}</if>
			<if test="auditTime != null">,`auditTime` = #{auditTime}</if>
			<if test="auditOpinion != null">,`auditOpinion` = #{auditOpinion}</if>
			<if test="auditStatus != null">,`auditStatus` = #{auditStatus}</if>
			<if test="isDelete != null">,`isDelete` = #{isDelete}</if>
			<if test="modifyUser != null">,`modifyUser` = #{modifyUser}</if>
			<if test="modifyDate != null">,`modifyDate` = #{modifyDate}</if>
		</set>
		where id = #{id}
	</update>

	<delete id="delete">
		delete from b2b_buyer_audit
		where id =	#{id}
	</delete>
	
</mapper>
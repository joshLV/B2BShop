<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.deepdragon.entity.weipu.BuyerOperatorMapper">

	<sql id="columnList">
		`id`,`tenantId`,`buyerId`,`userName`,`passwd`,`email`,`realName`,`mobile`,`roleValue`,`isvalid`,`isDelete`,`createUser`,`createDate`,`modifyUser`,`modifyDate`
	</sql>

	<select id="get" resultType="net.deepdragon.entity.weipu.BuyerOperator">
		select
		<include refid="columnList" />
		from ghj_buyer_operator
		where id = #{id}
	</select>

	<select id="find" resultType="net.deepdragon.entity.weipu.BuyerOperator">
		select
		<include refid="columnList" />
		from ghj_buyer_operator
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>

	<select id="findList" resultType="net.deepdragon.entity.weipu.BuyerOperator">
		select
		<include refid="columnList" />
		from ghj_buyer_operator
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>
	
	<select id="findListByIds" parameterType="java.lang.String"
		resultType="net.deepdragon.entity.weipu.BuyerOperator">
		select
		<include refid="columnList" />
		from ghj_buyer_operator
		where id in
		<foreach collection="list" item="id" index="index" open="("	close=")" separator=",">#{id}</foreach>
	</select>
	
	<select id="getAll" resultType="net.deepdragon.entity.weipu.BuyerOperator">
		select
		<include refid="columnList" />
		from ghj_buyer_operator 
		where tenantId = #{tenantId}
	</select>
	
	<select id="getPager" resultType="net.deepdragon.entity.weipu.BuyerOperator">
		select
		<include refid="columnList" />,DATE_FORMAT(createDate,'%Y-%m-%d %h:%i:%s') changeTime
		from ghj_buyer_operator 
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>
	
	<select id="getList" resultType="net.deepdragon.entity.weipu.BuyerOperator">
		select
		<include refid="columnList" />
		from ghj_buyer_operator 
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>

	<select id="getTotalCount" resultType="java.lang.Long">
		select count(id) as result
		from ghj_buyer_operator where tenantId = #{tenantId}
	</select>
	
	<select id="isExist" resultType="java.lang.Boolean">
		select count(id) as result from ghj_buyer_operator 
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>
	
	<insert id="save" parameterType="net.deepdragon.entity.weipu.BuyerOperator"
		useGeneratedKeys="true" keyProperty="id">
		insert into ghj_buyer_operator
		<trim prefix="(" suffix=")" suffixOverrides=",">
			`id`,`tenantId`
			<if test="buyerId != null">,`buyerId`</if>
			<if test="userName != null">,`userName`</if>
			<if test="passwd != null">,`passwd`</if>
			<if test="email != null">,`email`</if>
			<if test="realName != null">,`realName`</if>
			<if test="mobile != null">,`mobile`</if>
			<if test="roleValue != null">,`roleValue`</if>
			<if test="isvalid != null">,`isvalid`</if>
			<if test="isDelete != null">,`isDelete`</if>
			<if test="createUser != null">,`createUser`</if>
			<if test="createDate != null">,`createDate`</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			#{id},#{tenantId}
			<if test="buyerId != null">,#{buyerId}</if>
			<if test="userName != null">,#{userName}</if>
			<if test="passwd != null">,#{passwd}</if>
			<if test="email != null">,#{email}</if>
			<if test="realName != null">,#{realName}</if>
			<if test="mobile != null">,#{mobile}</if>
			<if test="roleValue != null">,#{roleValue}</if>
			<if test="isvalid != null">,#{isvalid}</if>
			<if test="isDelete != null">,#{isDelete}</if>
			<if test="createUser != null">,#{createUser}</if>
			<if test="createDate != null">,#{createDate}</if>
		</trim>
	</insert>

	<update id="update" parameterType="net.deepdragon.entity.weipu.BuyerOperator">
		update ghj_buyer_operator
		<set>
			`tenantId` = #{tenantId}
			<if test="buyerId != null">,`buyerId` = #{buyerId}</if>
			<if test="userName != null">,`userName` = #{userName}</if>
			<if test="passwd != null">,`passwd` = #{passwd}</if>
			<if test="email != null">,`email` = #{email}</if>
			<if test="realName != null">,`realName` = #{realName}</if>
			<if test="mobile != null">,`mobile` = #{mobile}</if>
			<if test="roleValue != null">,`roleValue` = #{roleValue}</if>
			<if test="isvalid != null">,`isvalid` = #{isvalid}</if>
			<if test="isDelete != null">,`isDelete` = #{isDelete}</if>
			<if test="modifyUser != null">,`modifyUser` = #{modifyUser}</if>
			<if test="modifyDate != null">,`modifyDate` = #{modifyDate}</if>
		</set>
		where id = #{id}
	</update>

	<delete id="delete">
		delete from ghj_buyer_operator
		where id =	#{id}
	</delete>
	
</mapper>
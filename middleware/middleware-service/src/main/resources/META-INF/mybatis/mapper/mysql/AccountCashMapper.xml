<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.deepdragon.entity.weipu.AccountCashMapper">

	<sql id="columnList">
		`id`,`tenantId`,`platform`,`userId`,`userName`,`cashType`,`cashNo`,`cashName`,`cashPhone`,`bankName`,`numberNo`,`bankProvince`,`bankCity`,`cashAccount`,`feeAccount`,`actualAccount`,`status`,`applyDate`,`approveOperator`,`approveDate`,`approveReason`,`remark`,`operator`,`operatorDate`,`createUser`,`createDate`,`modifyUser`,`modifyDate`
	</sql>

	<select id="get" resultType="net.deepdragon.entity.weipu.AccountCash">
		select
		<include refid="columnList" />
		from wp_account_cash
		where id = #{id}
	</select>


	<insert id="save" parameterType="net.deepdragon.entity.weipu.AccountCash"
		useGeneratedKeys="true" keyProperty="id">
		insert into wp_account_cash
		<trim prefix="(" suffix=")" suffixOverrides=",">
			`id`,`tenantId`
			<if test="platform != null">,`platform`</if>
			<if test="userId != null">,`userId`</if>
			<if test="userName != null">,`userName`</if>
			<if test="cashType != null">,`cashType`</if>
			<if test="cashNo != null">,`cashNo`</if>
			<if test="cashName != null">,`cashName`</if>
			<if test="cashPhone != null">,`cashPhone`</if>
			<if test="bankName != null">,`bankName`</if>
			<if test="numberNo != null">,`numberNo`</if>
			<if test="bankProvince != null">,`bankProvince`</if>
			<if test="bankCity != null">,`bankCity`</if>
			<if test="cashAccount != null">,`cashAccount`</if>
			<if test="feeAccount != null">,`feeAccount`</if>
			<if test="actualAccount != null">,`actualAccount`</if>
			<if test="status != null">,`status`</if>
			<if test="applyDate != null">,`applyDate`</if>
			<if test="createUser != null">,`createUser`</if>
			<if test="createDate != null">,`createDate`</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			#{id},#{tenantId}
			<if test="platform != null">,#{platform}</if>
			<if test="userId != null">,#{userId}</if>
			<if test="userName != null">,#{userName}</if>
			<if test="cashType != null">,#{cashType}</if>
			<if test="cashNo != null">,#{cashNo}</if>
			<if test="cashName != null">,#{cashName}</if>
			<if test="cashPhone != null">,#{cashPhone}</if>
			<if test="bankName != null">,#{bankName}</if>
			<if test="numberNo != null">,#{numberNo}</if>
			<if test="bankProvince != null">,#{bankProvince}</if>
			<if test="bankCity != null">,#{bankCity}</if>
			<if test="cashAccount != null">,#{cashAccount}</if>
			<if test="feeAccount != null">,#{feeAccount}</if>
			<if test="actualAccount != null">,#{actualAccount}</if>
			<if test="status != null">,#{status}</if>
			<if test="applyDate != null">,#{applyDate}</if>
			<if test="createUser != null">,#{createUser}</if>
			<if test="createDate != null">,#{createDate}</if>
		</trim>
	</insert>

	<update id="updateAccountCash" parameterType="net.deepdragon.entity.weipu.AccountCash">
		update wp_account_cash
			<set>
			<if test="status != null">,status = #{status}</if>
			<if test="modifyUser != null">,modifyUser = #{modifyUser}</if>
			<if test="modifyDate != null">,modifyDate = #{modifyDate}</if>
		</set>
		where id = #{id}
	</update>

	<select id="getList" resultType="net.deepdragon.entity.weipu.AccountCash">
		select
		<include refid="columnList" />
		from wp_account_cash
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>


</mapper>
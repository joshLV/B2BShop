		from ghj_vm_redenvelopaccount vr
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>

	<!--Desc:查询红包记录 Auth:zhangqiang Time:2016-03-09 15:50:16 Start-->
	<select id="getRedenvelopRecord" resultType="net.deepdragon.entity.weipu.GhjVmRedenvelopaccount">
		SELECT
		v.id,
		buyerId,
		groupId,
		redEnvelopType,
		redEnvelopId,
		createTime,
		expiryTime,
		amount,
		releaseSource,
		merchantId,
		CASE pIsAssigned
		WHEN 1 THEN '平台'
		WHEN 0 THEN m.name
		ELSE '平台'
		END AS merchantName,
		type,
		pIsAssigned AS isAssigned,
		pLimitation AS limitation,
		pSingleMaximum AS singleMaximum,
		pValidityPeriod AS validityPeriod
		FROM
		ghj_vm_redenvelopaccount v
		LEFT JOIN WP_MERCHANT M ON M.id = v.merchantId
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>

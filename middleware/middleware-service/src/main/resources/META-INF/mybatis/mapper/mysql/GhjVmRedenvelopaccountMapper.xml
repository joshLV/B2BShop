	<!-- 查询平台定向供应商的红包 -->
	<select id="getDirectionalMerchantList" resultType="net.deepdragon.entity.weipu.GhjVmRedenvelopaccount">
		select r.redEnvelopId AS id,vr.*
		from ghj_vm_redenvelopaccount vr
		left join ghj_redenvelop_rule r on vr.redEnvelopId = r.redEnvelopId
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>

	<select id="getRedenvelopList" resultType="net.deepdragon.entity.weipu.GhjVmRedenvelopaccount">

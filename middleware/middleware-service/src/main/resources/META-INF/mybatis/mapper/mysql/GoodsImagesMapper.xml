<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.deepdragon.entity.weipu.GoodsImagesMapper">

	<sql id="columnList">
		`id`,`tenantId`,`goodsId`,`title`,concat((select setvalue from shared_system_set where setKey='imageUrl'),concat(concat(groupName,'/'),url)) as `pic`,`groupName`,`url`,`imageWidth`,`imageHeight`,`orderNo`,`createUser`,`createDate`,`modifyUser`,`modifyDate`
	</sql>

	<select id="get" resultType="net.deepdragon.entity.weipu.GoodsImages">
		select
		<include refid="columnList" />
		from wp_goods_images
		where id = #{id}
	</select>

	<select id="find" resultType="net.deepdragon.entity.weipu.GoodsImages">
		select
		<include refid="columnList" />
		from wp_goods_images
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>

	<select id="findList" resultType="net.deepdragon.entity.weipu.GoodsImages">
		select
		<include refid="columnList" />
		from wp_goods_images
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>
	
	<select id="findListByIds" parameterType="java.lang.String"
		resultType="net.deepdragon.entity.weipu.GoodsImages">
		select
		<include refid="columnList" />
		from wp_goods_images
		where id in
		<foreach collection="list" item="id" index="index" open="("	close=")" separator=",">#{id}</foreach>
	</select>
	
	<select id="getAll" resultType="net.deepdragon.entity.weipu.GoodsImages">
		select
		<include refid="columnList" />
		from wp_goods_images 
		where tenantId = #{tenantId}
	</select>
	
	<select id="getPager" resultType="net.deepdragon.entity.weipu.GoodsImages">
		select
		<include refid="columnList" />
		from wp_goods_images 
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>
	
	<select id="getList" resultType="net.deepdragon.entity.weipu.GoodsImages">
		select
		<include refid="columnList" />
		from wp_goods_images 
		<if test="(@net.deepdragon.util.OgnlUtils@isEmpty(condition))!=null">${condition}</if>
	</select>

	<select id="getTotalCount" resultType="java.lang.Long">
		select count(id) as result
		from wp_goods_images where tenantId = #{tenantId}
	</select>
	
	<select id="isExist" resultType="java.lang.Boolean">
		select count(id) as result from wp_goods_images 
		where tenantId = #{tenantId} and ${property}=#{value}
	</select>
	
	<insert id="save" parameterType="net.deepdragon.entity.weipu.GoodsImages"
		useGeneratedKeys="true" keyProperty="id">
		insert into wp_goods_images
		<trim prefix="(" suffix=")" suffixOverrides=",">
			`id`,`tenantId`
			<if test="goodsId != null">,`goodsId`</if>
			<if test="title != null">,`title`</if>
			<if test="groupName != null">,`groupName`</if>
			<if test="url != null">,`url`</if>
			<if test="imageWidth != null">,`imageWidth`</if>
			<if test="imageHeight != null">,`imageHeight`</if>
			<if test="orderNo != null">,`orderNo`</if>
			<if test="createUser != null">,`createUser`</if>
			<if test="createDate != null">,`createDate`</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			#{id},#{tenantId}
			<if test="goodsId != null">,#{goodsId}</if>
			<if test="title != null">,#{title}</if>
			<if test="groupName != null">,#{groupName}</if>
			<if test="url != null">,#{url}</if>
			<if test="imageWidth != null">,#{imageWidth}</if>
			<if test="imageHeight != null">,#{imageHeight}</if>
			<if test="orderNo != null">,#{orderNo}</if>
			<if test="createUser != null">,#{createUser}</if>
			<if test="createDate != null">,#{createDate}</if>
		</trim>
	</insert>

	<update id="update" parameterType="net.deepdragon.entity.weipu.GoodsImages">
		update wp_goods_images
		<set>
			`tenantId` = #{tenantId}
			<if test="goodsId != null">,`goodsId` = #{goodsId}</if>
			<if test="title != null">,`title` = #{title}</if>
			<if test="groupName != null">,`groupName` = #{groupName}</if>
			<if test="url != null">,`url` = #{url}</if>
			<if test="imageWidth != null">,`imageWidth` = #{imageWidth}</if>
			<if test="imageHeight != null">,`imageHeight` = #{imageHeight}</if>
			<if test="orderNo != null">,`orderNo` = #{orderNo}</if>
			<if test="modifyUser != null">,`modifyUser` = #{modifyUser}</if>
			<if test="modifyDate != null">,`modifyDate` = #{modifyDate}</if>
		</set>
		where id = #{id}
	</update>

	<delete id="delete">
		delete from wp_goods_images
		where id =	#{id}
	</delete>
	
</mapper>
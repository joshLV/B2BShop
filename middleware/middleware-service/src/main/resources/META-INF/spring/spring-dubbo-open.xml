<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:tx="http://www.springframework.org/schema/tx"
	xmlns:aop="http://www.springframework.org/schema/aop" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd http://www.springframework.org/schema/tx
            http://www.springframework.org/schema/tx/spring-tx.xsd http://www.springframework.org/schema/aop
            http://www.springframework.org/schema/aop/spring-aop.xsd http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context.xsd">

	<description>开放平台接口定义</description>

	<!-- 开放平台模块服务 -->
	<dubbo:service interface="net.deepdragon.service.open.ModuleService"
		ref="openModuleServiceImpl" version="${open.service.version}" group="${open.service.group}"
		timeout="${open.service.timeout}" retries="${open.service.retries}" />

	<dubbo:service interface="net.deepdragon.service.open.WalletService"
				   ref="openWalletServiceImpl" version="${open.service.version}" group="${open.service.group}"
				   timeout="${open.service.timeout}" retries="${open.service.retries}" />
</beans>